{
  "name": "lofi-player",
  "version": "1.0.0",
  "description": "A Lofi Player builded with HTML, CSS and Javascript using Parcel as Module Bundler",
  "main": "index.js",
  "pwaManifest": {
    "name": "Lofi Player",
    "shortName": "L-Player",
    "startURL": "/lofi-player/",
    "theme": "#020402",
    "generateIconOptions": {
      "baseIcon": "./public/favicon.svg",
      "sizes": [
        32,
        64,
        192,
        384,
        512,
        780
      ],
      "genFavicons": true
    }
  },
  "scripts": {
    "dev": "parcel public/index.html --out-dir development -p 3000",
    "pre-deploy": "parcel build public/index.html --out-dir dist --public-url /lofi-player && workbox generateSW",
    "deploy": "node publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaksCastro/lofi-player.git"
  },
  "keywords": [
    "parcel"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LaksCastro/lofi-player/issues"
  },
  "homepage": "https://lakscastro.github.io/lofi-player",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-pwa-manifest": "^3.0.6",
    "sass": "^1.26.3",
    "workbox-cli": "^5.1.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "gh-pages": "^2.2.0"
  }
}
